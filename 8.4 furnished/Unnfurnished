import pandas as pd
import matplotlib.pyplot as plt

df = pd.DataFrame([['Furnished','Apartment',10],['Furnished','House',12],['Furnished','Flat',13],['Furnished','Studio',8],
                   ['UnFurnished','Apartment',8],['UnFurnished','House',7],['UnFurnished','Flat',13], ['UnFurnished','Studio',4],],columns=['group','column','val'])

df.pivot("column", "group", "val").plot(kind='bar')

plt.show()
