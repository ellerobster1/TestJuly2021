import matplotlib.pyplot as plt; plt.rcdefaults()
import numpy as np
import matplotlib.pyplot as plt

objects = ('0 bed','1 bed','2 bed','3 bed','4 bed','5 bed','6 bed','7 bed','9 bed')
y_pos = np.arange(len(objects))
performance = [158,1380,936,191,35,11,4,1,2]

plt.bar(y_pos, performance, align='center', alpha=0.5)
plt.xticks(y_pos, objects)
plt.ylabel('Quantity')
plt.title('Number of bedrooms in a Property')

plt.show()
